        <xs:complexType sawsdl:modelReference="http://www.w3.org/2000/01/rdf-schema#Resource Resource">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              Resource - Resource represents any resource, physical or digital, that is related to the described dataset. 
            </xs:documentation>
          </xs:annotation>
          <xs:sequence>
            <xs:element minOccurs="0" ref="c:iri"/>
            <xs:element minOccurs="0" name="relation_type" type="xs:anyURI" sawsdl:modelReference="relationType">
              <xs:annotation>
                <xs:documentation xml:lang="en">
                  Relation type - Type of the relation between Dataset and related resource.
                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="type" type="xs:anyURI" sawsdl:modelReference="resourceType http://purl.org/dc/terms/type">
              <xs:annotation>
                <xs:documentation xml:lang="en">
                  Type - A description of the resource. Gets the value from controlled list https://datacite-metadata-schema.readthedocs.io/en/4.6/appendices/appendix-1/resourceTypeGeneral/.
                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="title" type="xs:string" sawsdl:modelReference="relatedItemTitle http://purl.org/dc/terms/title">
              <xs:annotation>
                <xs:documentation xml:lang="">
                  Title
                </xs:documentation>
                <xs:documentation xml:lang="en">
                  A name given to the Catalogue Record.
                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="identifier" type="xs:anyURI" sawsdl:modelReference="identifier http://purl.org/dc/terms/identifier">
              <xs:annotation>
                <xs:documentation xml:lang="en">
                  Identifier - An unambiguous reference to the resource within a given context.
                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="qualified_relations" sawsdl:modelReference="qualifiedRelation">
              <xs:annotation>
                <xs:documentation xml:lang="en">
                  qualified relation - Attachment of resource to the relationship.
                </xs:documentation>
              </xs:annotation>
              <xs:complexType sawsdl:modelReference="http://www.w3.org/ns/dcat#Relationship">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Resource to Agent Relationship - An association class for attaching additional information to a relationship between Dataset and Agent.
                  </xs:documentation>
                </xs:annotation>
                <xs:sequence>
                  <xs:element minOccurs="0" ref="c:iri"/>
                  <xs:element maxOccurs="unbounded" name="roles" sawsdl:modelReference="http://www.w3.org/ns/dcat#hadRole">
                    <xs:annotation>
                      <xs:documentation xml:lang="cs">
                        sehraná role - Funkce entity či agenta ve vztahu k jiné entitě či zdroji.
                      </xs:documentation>
                      <xs:documentation xml:lang="da">
                        havde rolle - Den funktion en entitet eller aktør har i forhold til en anden ressource.
                      </xs:documentation>
                      <xs:documentation xml:lang="en">
                        had role - The function of an entity or agent with respect to another entity or resource.
                      </xs:documentation>
                      <xs:documentation xml:lang="es">
                        La función de una entidad o agente con respecto a otra entidad o recurso.
                      </xs:documentation>
                      <xs:documentation xml:lang="it">
                        tiene rol - La funzione di un'entità o un agente rispetto ad un'altra entità o risorsa.
                      </xs:documentation>
                    </xs:annotation>
                    <xs:complexType sawsdl:modelReference="http://www.w3.org/2004/02/skos/core#Concept">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">
                          Resource agent role type - An idea or notion; a unit of thought.
                        </xs:documentation>
                      </xs:annotation>
                      <xs:sequence>
                        <xs:element minOccurs="0" ref="c:iri"/>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" name="relations" sawsdl:modelReference="http://purl.org/dc/terms/relation">
                    <xs:annotation>
                      <xs:documentation xml:lang="en">
                        relation
                      </xs:documentation>
                    </xs:annotation>
                    <xs:complexType sawsdl:modelReference="http://xmlns.com/foaf/0.1/Agent">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">
                          Agent - Any entity carrying out actions with respect to the entities Catalogue and the Catalogued Resources.
                        </xs:documentation>
                      </xs:annotation>
                      <xs:sequence>
                        <xs:element minOccurs="0" ref="c:iri"/>
                        <xs:element minOccurs="0" maxOccurs="unbounded" name="external_identifiers" type="xs:string" sawsdl:modelReference="agentExternalIdentifier">
                          <xs:annotation>
                            <xs:documentation xml:lang="en">
                              External identifier - External identifer of an Agent.
                            </xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" maxOccurs="unbounded" name="contact_points" sawsdl:modelReference="http://www.w3.org/ns/dcat#contactPoint">
                          <xs:annotation>
                            <xs:documentation xml:lang="ar">
                              عنوان اتصال - تربط قائمة البيانات بعنوان اتصال موصف  باستخدام VCard
                            </xs:documentation>
                            <xs:documentation xml:lang="cs">
                              kontaktní bod - Relevantní kontaktní informace pro katalogizovaný zdroj. Doporučuje se použít slovník VCard.
                            </xs:documentation>
                            <xs:documentation xml:lang="da">
                              kontaktpunkt - Relevante kontaktoplysninger for den katalogiserede ressource. Anvendelse af vCard anbefales.
                            </xs:documentation>
                            <xs:documentation xml:lang="el">
                              σημείο επικοινωνίας - Συνδέει ένα σύνολο δεδομένων με ένα σχετικό σημείο επικοινωνίας, μέσω VCard.
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                              contact point - Relevant contact information for the catalogued resource. Use of vCard is recommended.
                            </xs:documentation>
                            <xs:documentation xml:lang="es">
                              Punto de contacto - Información relevante de contacto para el recurso catalogado. Se recomienda el uso de vCard.
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                              point de contact - Relie un jeu de données à une information de contact utile en utilisant VCard.
                            </xs:documentation>
                            <xs:documentation xml:lang="it">
                              punto di contatto - Informazioni di contatto rilevanti per la risorsa catalogata. Si raccomanda l'uso di vCard.
                            </xs:documentation>
                            <xs:documentation xml:lang="ja">
                              窓口 - データセットを、VCardを用いて提供されている適切な連絡先情報にリンクします。
                            </xs:documentation>
                          </xs:annotation>
                          <xs:complexType sawsdl:modelReference="ContactDetails">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">
                                Contact details - Contact details such as telephone, e-mail address etc.
                              </xs:documentation>
                            </xs:annotation>
                            <xs:sequence>
                              <xs:element minOccurs="0" ref="c:iri"/>
                              <xs:element minOccurs="0" maxOccurs="unbounded" name="phone" type="xs:string" sawsdl:modelReference="dataBox">
                                <xs:annotation>
                                  <xs:documentation xml:lang="en">
                                    Phone - Code of the data box.
                                  </xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element minOccurs="0" maxOccurs="unbounded" name="email" type="xs:string" sawsdl:modelReference="email">
                                <xs:annotation>
                                  <xs:documentation xml:lang="en">
                                    Email - Email address.
                                  </xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element minOccurs="0" maxOccurs="unbounded" name="data_box" type="xs:string" sawsdl:modelReference="phone">
                                <xs:annotation>
                                  <xs:documentation xml:lang="en">
                                    Data box - Phone number
                                  </xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element minOccurs="0" maxOccurs="unbounded" name="addresses" sawsdl:modelReference="http://www.w3.org/2006/vcard/ns#hasAddress">
                                <xs:annotation>
                                  <xs:documentation xml:lang="en">
                                    has address - To specify the components of the delivery address for the object
                                  </xs:documentation>
                                </xs:annotation>
                                <xs:complexType sawsdl:modelReference="http://www.w3.org/ns/locn#Address">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">
                                      Address - An &#34;address representation&#34; as conceptually defined by the INSPIRE Address Representation data type. The locn:addressId property may be used to link this locn:Address to other representations.
                                    </xs:documentation>
                                  </xs:annotation>
                                  <xs:sequence>
                                    <xs:element minOccurs="0" ref="c:iri"/>
                                  </xs:sequence>
                                </xs:complexType>
                              </xs:element>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="time_references" sawsdl:modelReference="hasTimeReference">
              <xs:annotation>
                <xs:documentation xml:lang="en">
                  has time reference
                </xs:documentation>
              </xs:annotation>
              <xs:complexType sawsdl:modelReference="TimeReference">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Time reference - A date on which something relative to the resource has happened.
                  </xs:documentation>
                </xs:annotation>
                <xs:sequence>
                  <xs:element minOccurs="0" ref="c:iri"/>
                  <xs:element minOccurs="0" name="date_information" type="xs:string" sawsdl:modelReference="dateInformation">
                    <xs:annotation>
                      <xs:documentation xml:lang="">
                        Date information
                      </xs:documentation>
                      <xs:documentation xml:lang="en">
                        Additional information to the date in text form.
                      </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="date_type" type="xs:anyURI" sawsdl:modelReference="dateType">
                    <xs:annotation>
                      <xs:documentation xml:lang="">
                        Date type
                      </xs:documentation>
                      <xs:documentation xml:lang="en">
                        Type of the date.
                      </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="date" type="xs:date" sawsdl:modelReference="dateValue">
                    <xs:annotation>
                      <xs:documentation xml:lang="en">
                        Date - A point or period of time associated with an event in the lifecycle of the resource.
                      </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="alternate_identifiers" sawsdl:modelReference="hasAlternateIdentifier">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            has alternate identifier
          </xs:documentation>
        </xs:annotation>
        <xs:complexType sawsdl:modelReference="AlternateIdentifier">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              Alternate identifier - An identifier other than the primary Identifier applied to the resource being registered. This may be any alphanumeric string which is unique within its domain of issue. May be used for local identifiers, a serial number of an instrument or an inventory number. The AlternateIdentifier should be an additional identifier for the same instance of the resource (i.e., same location, same file).
            </xs:documentation>
          </xs:annotation>
          <xs:sequence>
            <xs:element minOccurs="0" ref="c:iri"/>
            <xs:element minOccurs="0" name="alternate_identifier_type" type="xs:string" sawsdl:modelReference="alternateIdentifierType">
              <xs:annotation>
                <xs:documentation xml:lang="en">
                  alternate identifier type - The type of the Alternate identifier.
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="datová_sada" type="datová_sada"/>
</xs:schema>
